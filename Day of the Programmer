year = int(input())
if year > 1699 and year < 2701:
    if year > 1918:
        if year%400 == 0 or (year%4 == 0 and year%100 != 0):
            print('12.09.', year, sep = '')
        else:
            print('13.09.', year, sep = '')

    if year < 1918:
        if year%4 == 0:
            print('12.09.', year, sep = '')
        else:
            print('13.09.', year, sep = '')
    if year == 1918:
        print('26.09.1918')
        
